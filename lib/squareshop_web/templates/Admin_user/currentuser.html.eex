<div class="sq-main-layout">
	<div class="sq-panel sq-admin-bruker-listepanel sq-black"  style="overflow-y: auto;">
		<table class="sq-tables" width="100%">
			<thead>
				<tr>
					<th>Kundenr</th>
					<th>Fornavn</th>
					<th>Etternavn</th>
					<th>Telefon</th>
					<th>E-post</th>
					<th>Address</th>
					<th>Zip Code</th>
					<th>City</th>
					<th>Country</th>

				</tr>
			</thead>
			<tbody>
				<%= for user <- @users do %>
				<tr>
					<td> <%= user.id %> </td>
					<td> <%= user.fname %>  </td>
					<td> <%= user.lname %></td>
					<td> <%= user.phone %></td>
					<td> <%= user.email %></td>
					<td> <%= user.address %>  </td>
					<td> <%= user.zip_code %></td>
					<td> <%= user.city %></td>
					<td> <%= user.country %></td>
				</tr>
				<% end %>
			</tbody>

	  </table>
	</div>
